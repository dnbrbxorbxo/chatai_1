{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "api/fetch-openai.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/fetch-openai",
      "dest": "/api/fetch-openai.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
