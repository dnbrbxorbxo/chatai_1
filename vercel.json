{
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/fetch-openai.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
